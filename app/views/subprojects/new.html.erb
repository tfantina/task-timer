<%= form_for(@subproject, method: :post, remote: true, html: {class: "form-group"}) do |f| %>
  <%= f.label "Name subproject"%>
  <%= f.text_field :name, class: "form-control" %>
  <br />
  <%= f.label "Describe subproject" %>
  <%= f.text_field :description, class: "form-control" %>

  <br />


  <%= f.submit "Insert", class: 'btn btn-primary' %>
<% end %>
